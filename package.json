{
  "name": "blu-j-ts-validate",
  "version": "1.0.0",
  "repository": "git@github.com:Blu-J/ts-validate-json-schema.git",
  "author": "Justin Miller <dragondef@gmail.com>",
  "license": "MIT",
  "main": "lib/matches.js",
  "types": "lib/matches.d.ts",
  "files": [
    "lib/"
  ],
  "dependencies": {
    "ts-matches": "^3.2.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.8",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "ci": "npm run build && npm run test && npm run lint && npm run pretty-check",
    "build": "tsc",
    "test": "jest",
    "lint": "tslint --project tsconfig.json --format stylish",
    "pretty-check": "prettier --check src",
    "fmt": "prettier --write src"
  },
  "jest": {
    "testRegex": "/src/.*.spec.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
